# Database
DATABASE_URL="postgresql://user:password@localhost:5432/propertyiq?schema=public"
REDIS_URL="redis://localhost:6379"

# Server
NODE_ENV=development
PORT=4000
CLIENT_URL=http://localhost:3000

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# ML Service
ML_SERVICE_URL=http://localhost:8000

# External APIs
MAPBOX_ACCESS_TOKEN=your-mapbox-access-token
ZILLOW_API_KEY=your-zillow-api-key
REALTOR_API_KEY=your-realtor-api-key
WALK_SCORE_API_KEY=your-walk-score-api-key
GREAT_SCHOOLS_API_KEY=your-great-schools-api-key
CENSUS_API_KEY=your-census-api-key

# OpenAI (for property descriptions and analysis)
OPENAI_API_KEY=your-openai-api-key

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Caching
CACHE_TTL_SECONDS=3600
PROPERTY_CACHE_TTL=1800
MARKET_DATA_CACHE_TTL=86400

# Background Jobs
BULLMQ_REDIS_URL=redis://localhost:6379
JOB_CONCURRENCY=5

# Logging
LOG_LEVEL=info
